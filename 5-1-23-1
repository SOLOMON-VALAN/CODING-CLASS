n=int(input("Enter number of rows and columns :" ))
m=[]
res=[]
for i in range(0,n):
    l=[]
    for j in range(0,n):
        x=int(input())
        l.append(x)
    m.append(l)
k=int(input('Enter a number : '))
for j in range(0,n):
    for i in range(0,n):
        if i==j:
            x=m[i][j]
            res.append(x)
        if i+j==n-1:
            x=m[i][j]
            res.append(x)
for j in range(0,n):
    for i in range(0,n):
        print(m[i][j],end='  ')
    print()
