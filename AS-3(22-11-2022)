Student_number=eval(input("Enter student ID : "))
Student_name=input("Enter name of the student : ")
Student_gender=input("Enter student gender : ")
Student_group=input("Enter group : ")
Student_address=input("Enter address : ")
Student_phno=eval(input("Enter student phone number : "))
print("ID : ",Student_number)
print("Name : ",Student_name)
print("Gender : ",Student_gender)
print("Group : ",Student_group)
print("Address : ",Student_address)
print("Mobile number : ",Student_phno)
