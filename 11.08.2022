#include<stdio.h>
int main()
{
    int n,sum=0,r,temp;
    printf("Enter the number:");
    scanf("%d",&n);
    temp=n;
    while(n!=0)
    {
        r=n%10;
        sum=sum+r;
        n=n/10;
    }
    n=temp;
    if(n%sum==0)
    {
        printf("Harshad number\n");
    }
    else
    {
        printf("Not a Harshad number");
    }
    return 0;
}
