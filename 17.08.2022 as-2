#include<stdio.h>
int main()
{
    int a,b,c,i,terms;
    printf("Enter the number of terms:\n");
    scanf("%d",&terms);
    a=0;
    b=1;
    c=0;
    printf("Fabinocci series:\n");
    for(i=0;i<terms;i++)
    {
        printf("%d\t",c);
        a=b;
        b=c;
        c=a+b;
    }
    return 0;
}
